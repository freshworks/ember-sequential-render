{
  "name": "@freshworks/ember-sequential-render",
  "version": "2.0.0",
  "description": "An elegant solution to optimize your application's render performance.",
  "keywords": [
    "ember-addon",
    "sequential-render",
    "Critical Rendering Path",
    "render performance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/freshworks/ember-sequential-render.git"
  },
  "license": "MIT",
  "author": "Kanalveli Ramachandran <kanalveli.ramachandran@freshworks.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint **/*.scss",
    "lint": "yarn lint:js && yarn lint:hbs"
  },
  "lint-staged": {
    "*.js": "eslint . --fix --ignore-path .gitignore",
    "*.hbs": "ember-template-lint .",
    "*.scss": "stylelint **/*.scss"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-concurrency": "2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "~3.28.4",
    "ember-cli-addon-docs": "^4.0.3",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylelint": "^3.0.2",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^3.28.3",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-qunit-custom-assertions": "^2.0.1",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^5.0.0",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "lint-staged": "^10.0.7",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "webpack": "^5.64.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://freshworks.github.io/ember-sequential-render"
}
